{
  "expo": {
    "name": "portfolio-app",
    "slug": "portfolio-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/app.png",
      "resizeMode": "contain"
      // "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app.png"
        // "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.roushan0209.portfolioapp"
    },
    "web": {
      "favicon": "./assets/app.png"
    },
    "plugins": [
      [
        "expo-sensors",
        {
          "motionPermission": "Allow sensor to access your device motion"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "422a1756-deeb-418e-86cf-0ffb561770fc"
      }
    }
  }
}
